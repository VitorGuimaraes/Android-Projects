<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.com.dolker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
            android:label="Sign Up" />
        <activity
            android:name=".MainActivity"
            android:label="Dolker" />
        <activity
            android:name=".RegisterDogActivity"
            android:label="Register Dog" />
        <activity
            android:name=".DogsListActivity"
            android:label="Dogs List" />
        <activity
            android:name=".RidesActivity"
            android:label="Rides" />
        <activity
            android:name=".CallDolkerActivity"
            android:label="Call a Dolker" />
        <activity
            android:name=".FindAccount"
            android:label="Find Account" />
        <activity
            android:name=".RecoverAccountActivity"
            android:label="Recover Account" />
        <activity
            android:name=".OptionsActivity"
            android:label="Options"></activity>
    </application>

</manifest>